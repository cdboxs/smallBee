/* pages/feedback/index.wxss */
page{
  background:#fff;
}
.feedback{
  display:block;
  width:100%;
  margin:0 auto;
  overflow: hidden;
  padding:30rpx;
  box-sizing: border-box;
}
.feedback textarea{
  display: block;
  width:100%;
  height:410rpx;
  background:#eee;
  padding:20rpx;
  box-sizing: border-box;
  border:1rpx solid #ddd;
  border-radius: 5px;
  font-size: 30rpx;
  color:#666;
}
.feedback button{
  color:#fff;
  margin:35rpx auto;
  background:#ffc72a;
}



















