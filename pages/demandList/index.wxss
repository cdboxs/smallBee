/* pages/merchantList/index.wxss */
page{
  background:#fff;
}
.mtop{
  margin:78rpx 0 0 0;
}
.ynData{
  width:100%;
  margin:100rpx 0;
  text-align: center;
  font-size: 35rpx;
  color:#363636;
}
.dropDown{
  width:100%;
  padding:15rpx;
  background:#fff;
  box-shadow: 0px 2px 10px 1px #fdfbfb;
  position: fixed;top:0;left:0;z-index:99999;
  display: flex;
  align-items: center;
  box-sizing: border-box;
}
.dropDown text{
  color:#555;
  display: block;
  font-size: 30rpx;
}
.dropDown image{
  display: block;
  width:32rpx;
  height:32rpx;
}
.dropInfo{
  background:#fff;
  position: fixed;top:68rpx;left:0;z-index:99999;
  display: flex;
  flex-flow: row wrap;
 
}

.dropInfo text{
  display: block;
  width: 25%;
  color:#888;
  padding:30rpx 0;
  text-align: center;
  font-size: 30rpx;
  margin:0 -1px -1px 0;
  border-right:1rpx solid #ddd;
  border-bottom:1rpx solid #ddd;
}





















