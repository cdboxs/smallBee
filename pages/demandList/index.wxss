/* pages/merchantList/index.wxss */
page{
  background:#fff;
}
.mtop{
  margin:78rpx 0 0 0;
}
.ynData{
  width:100%;
  margin:100rpx 0;
  text-align: center;
  font-size: 35rpx;
  color:#363636;
}
.dropDown{
  width:100%;
  height:76rpx;
  background:#fff;
  box-shadow: 0px 2px 10px 1px #fdfbfb;
  position: fixed;top:0;left:0;z-index:99999;
  display: flex;
  align-items: center;
  padding-left:35rpx;
}
.dropDown text{
  color:#555;
  display: block;
  font-size: 30rpx;
}
.dropDown image{
  display: block;
  width:32rpx;
  height:32rpx;
}
.dropInfo{
  background:#fff;
  position: fixed;top:76rpx;left:0;z-index:99999;
  display: flex;
  padding:25rpx 0;
}
.dropInfo scroll-view{
  width:180rpx;
  background:#fefefe;
  border-right:1rpx solid #eee;
}
.dropInfo scroll-view text{
  display: block;
  text-align: center;
  padding:25rpx 0;
  font-size: 30rpx;
  color:#333;
}
.dropInfo view text{
  display: block;
  text-align: center;
  padding:25rpx 100rpx;
  font-size: 30rpx;
  color:#333;
}





















