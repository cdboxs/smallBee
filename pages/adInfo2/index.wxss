/* pages/adInfo2/index.wxss */
/* pages/checkIn/index.wxss */
page{
  background:#fff;
}
.welcomeIN{
  width:100%;
  height: 120rpx;
  line-height: 120rpx;
  text-align: center;
}
.checkInF{
  display: block;
  width:92%;
  margin:0 auto;
}
.checkInF view{
  padding:20rpx 0;
  display: flex;
  flex-flow: row wrap;
  align-items: center;
  position: relative;
  border-bottom: 1rpx solid #eee;
}
.checkInF view .tipx{
  color:red;
  font-size: 46rpx;
  position: absolute;top:23rpx;right:20rpx;
}
.checkInF view.times{
  padding:20rpx 0;
  display: flex;
  flex-flow: row;
  align-items: center;
  border-bottom: 1rpx solid #eee;
}
.checkInF view label{
  font-size: 30rpx;
}
.checkInF view input,.checkInF view textarea{
  color: #555;
  font-size: 27rpx;
}
.checkInSend{
  color:#fff;
  background:#ffc72a;
  margin:36rpx 0;
}
.checkInF view.picAdd{
  width: 100%;
  border:none;
}
.checkInF view .addPic{
  display: block;
  width:128rpx;
  height: 128rpx;
  margin:10rpx;
}
.checkInF view picker{
   width:30%;
}
.checkInF view picker input{
  display: block;
  width:100%;
  margin:0 10rpx;
  text-align: center;
}
.checkInF view textarea{
  box-sizing: border-box;
  padding:15rpx;
  height: 250rpx;
  line-height: 50rpx;
}
.callPhone{
  width:48rpx;
  height:48rpx;
}

.checkInF .adinfo2{
  width:100%;
  margin:0 auto;
  border:0;
}
.checkInF .adinfo2 label{
  display: block;
  width: 100%;
}
.checkInF .adinfo2 view{
  border:0;
}











